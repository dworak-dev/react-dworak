{
  "name": "react-dworak",
  "version": "1.2.3",
  "devDependencies": {
    "depcheck": "^1.4.7",
    "prettier": "3.6.2",
    "syncpack": "^13.0.4",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "yarn@1.22.22",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "depcheck": "depcheck --ignores='typescript,prettier'",
    "dev": "turbo run dev",
    "docker:default-app:build": "docker build -t default-app -f apps/default-app/Dockerfile . --debug",
    "docker:default-app:run": "docker run --name my-default-app --rm -p 3000:3000 default-app",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "sync": "turbo run sync",
    "syncpack:format": "syncpack format",
    "syncpack:mismatch": "syncpack list-mismatches --types prod"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
